{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "google"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "max-len": ["error", { "code": 160, "ignoreRegExpLiterals": true }],
        "require-jsdoc" : 0,
        "new-cap": ["error", { "properties": false }]
    }
}
